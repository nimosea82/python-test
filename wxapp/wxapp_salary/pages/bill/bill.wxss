.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  /*padding: 200rpx 0;*/
  box-sizing: border-box;

}

.modal_container{
  display: flex;
  flex-direction: column;
  margin-top: 100rpx;
  padding: 20rpx;
  background: #F0F4F3;
  height: 480rpx;
}

.modal_title{
  color: black;
  margin: 5px;
  text-align: center;
}

.modal_content{
  margin: 5px;
  padding: 5px;
}
.modal_c1{
  display: flex;
  flex-direction: row;
}

.modal_left{
  width: 150rpx;
  font-size: small;
  color: rgb(63, 63, 63);
}

.modal_right{
  font-size: smaller;
  color: gray;
}

.modal_bottom{
  color: rgb(184, 182, 182);
  margin: 5px;
  text-align: center;
  font-size: 15px;

}

.modal_bottom_box{
  margin-top:150rpx ;

  

}
